{
  "name": "browser-game-template",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --no-autoinstall",
    "build": "rm -rf dist && parcel build src/index.html --public-url ./",
    "typecheck": "tsc --module commonjs --allowJs --checkJs --noEmit --target es2016 src/*.js",
    "lint": "eslint .",
    "validate-ci": "circleci config validate",
    "check-all": "yarn lint && yarn typecheck && yarn validate-ci",
    "munch": "muncher --input asset --output static/spritesheet/spritesheet --flip"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "muncher": "^0.0.5",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "howler": "^2.1.2",
    "juice.js": "^1.0.6",
    "l1": "^0.6.0-3",
    "lodash": "^4.17.15",
    "pixi.js": "^5.1.2"
  }
}
