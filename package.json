    "execa": "7.2.0",
    "fs-extra": "11.1.1",
    "gradient-string": "2.0.2",
    "howler": "2.2.3",
    "listr2": "6.6.1",
    "load-json-file": "7.0.1",
    "nano-panel": "0.2.0",
