{
  "name": "alchemy-engine",
  "version": "0.1.0",
  "description": "Create 2D browser games with pixi.js",
  "keywords": [],
  "homepage": "https://github.com/sajmoni/alchemy#readme",
  "bugs": {
    "url": "https://github.com/sajmoni/alchemy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/alchemy.git"
  },
  "license": "MIT",
  "author": "sajmoni",
  "type": "module",
  "files": [
    "dist/",
    "template/"
  ],
  "bin": {
    "alc": "dist/src/cli/index.js"
  },
  "main": "dist/src/lib/index.js",
  "prettier": "prettier-config-one",
  "scripts": {
    "s": "tsx .",
    "build": "rm -rf dist && tsc",
    "go": "rm -rf game && npm run build && chmod +x dist/src/cli/index.js && node dist/src/cli/index.js create game && cpy vite.dev.config.ts game --rename=vite.config.ts && cd game && npm run dev",
    "test": "vitest --dir src/lib --dom",
    "test:cli": "npm run build && chmod +x dist/src/cli/index.js && vitest --dir src/cli",
    "release": "npm run build && np --no-tests"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/gradient-string": "1.1.5",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "cpy-cli": "5.0.0",
    "get-bin-path": "11.0.0",
    "happy-dom": "13.8.6",
    "np": "10.0.1",
    "pixi.js": "8.0.2",
    "prettier": "3.2.5",
    "prettier-config-one": "0.2.1",
    "tempy": "3.1.0",
    "tsconfig-one": "0.0.9",
    "tsx": "4.7.1",
    "vitest": "1.4.0"
  },
  "peerDependencies": {
    "pixi.js": "8.x"
  },
  "dependencies": {
    "@types/howler": "2.2.11",
    "cac": "6.7.14",
    "chalk": "5.3.0",
    "chokidar": "3.6.0",
    "execa": "8.0.1",
    "fs-extra": "11.2.0",
    "gradient-string": "2.0.2",
    "howler": "2.2.4",
    "listr2": "8.0.2",
    "load-json-file": "7.0.1",
    "nano-panel": "0.2.2",
    "nano-pool": "0.1.0",
    "park-miller": "2.0.0",
    "react": "18.2.0",
