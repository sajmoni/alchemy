{
  "name": "alchemy-engine",
  "version": "0.0.28",
  "description": "Create 2D browser games with pixi.js and TypeScript",
  "keywords": [],
  "homepage": "https://github.com/sajmoni/alchemy#readme",
  "bugs": {
    "url": "https://github.com/sajmoni/alchemy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/alchemy.git"
  },
  "license": "MIT",
  "author": "sajmoni",
  "type": "module",
  "files": [
    "dist/",
    "template/"
  ],
  "bin": {
    "alc": "dist/src/cli/index.js"
  },
  "main": "dist/src/lib/index.js",
  "prettier": "prettier-config-one",
  "scripts": {
    "s": "tsx .",
    "build": "rm -rf dist && tsc",
    "go": "rm -rf game && npm run build && chmod +x dist/src/cli/index.js && node dist/src/cli/index.js create game && cpy vite.dev.config.ts game --rename=vite.config.ts && cd game && npm run dev",
    "test": "vitest --dir src/lib --dom",
    "test:cli": "npm run build && chmod +x dist/src/cli/index.js && vitest --dir src/cli",
    "release": "npm run build && np --no-tests"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/gradient-string": "1.1.2",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "cpy-cli": "5.0.0",
    "get-bin-path": "10.0.0",
    "happy-dom": "10.10.4",
    "np": "8.0.4",
